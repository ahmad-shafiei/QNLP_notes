\PYG{c+c1}{\PYGZsh{} Recreate the same environment from the YAML file}
conda\PYG{+w}{ }env\PYG{+w}{ }create\PYG{+w}{ }\PYGZhy{}f\PYG{+w}{ }qnlp\PYGZus{}environment.yml
\PYG{c+c1}{\PYGZsh{} Remove the qnlp environment completely}
conda\PYG{+w}{ }remove\PYG{+w}{ }\PYGZhy{}n\PYG{+w}{ }qnlp\PYG{+w}{ }\PYGZhy{}\PYGZhy{}all
\end{MintedVerbatim}
